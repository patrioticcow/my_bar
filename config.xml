<?xml version='1.0' encoding='utf-8'?>
<widget id="com.massinflux.mybar" version="1.2.5" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://cordova.apache.org/ns/1.0 ">
    <name>My Bar</name>
    <description>My Bar</description>
    <author email="admin@massinflux.com" href="https://massinflux.com">Massinflux</author>
    <content src="index.html" />
    <access allows-arbitrary-loads-for-media="true" allows-arbitrary-loads-in-web-content="true" allows-local-networking="true" origin="*" />
    <allow-navigation href="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
  <plugin name="cordova-plugin-whitelist" spec="^1.3.3"/>
  <plugin name="cordova-plugin-splashscreen" spec="^5.0.2"/>
  <plugin name="cordova-plugin-device" spec="^2.0.2"/>
  <engine name="android" spec="^7.0.0"/>
  <platform name="android">
    <allow-intent href="market:*"/>
    <icon qualifier="ldpi" src="res/android/icon/drawable-ldpi-icon.png"/>
    <icon qualifier="mdpi" src="res/android/icon/drawable-mdpi-icon.png"/>
    <icon qualifier="hdpi" src="res/android/icon/drawable-hdpi-icon.png"/>
    <icon qualifier="xhdpi" src="res/android/icon/drawable-xhdpi-icon.png"/>
    <icon qualifier="xxhdpi" src="res/android/icon/drawable-xxhdpi-icon.png"/>
    <icon qualifier="xxxhdpi" src="res/android/icon/drawable-xxxhdpi-icon.png"/>
    <splash qualifier="land-ldpi" src="res/android/splash/drawable-land-ldpi-screen.png"/>
    <splash qualifier="land-mdpi" src="res/android/splash/drawable-land-mdpi-screen.png"/>
    <splash qualifier="land-hdpi" src="res/android/splash/drawable-land-hdpi-screen.png"/>
    <splash qualifier="land-xhdpi" src="res/android/splash/drawable-land-xhdpi-screen.png"/>
    <splash qualifier="land-xxhdpi" src="res/android/splash/drawable-land-xxhdpi-screen.png"/>
    <splash qualifier="land-xxxhdpi" src="res/android/splash/drawable-land-xxxhdpi-screen.png"/>
    <splash qualifier="port-ldpi" src="res/android/splash/drawable-port-ldpi-screen.png"/>
    <splash qualifier="port-mdpi" src="res/android/splash/drawable-port-mdpi-screen.png"/>
    <splash qualifier="port-hdpi" src="res/android/splash/drawable-port-hdpi-screen.png"/>
    <splash qualifier="port-xhdpi" src="res/android/splash/drawable-port-xhdpi-screen.png"/>
    <splash qualifier="port-xxhdpi" src="res/android/splash/drawable-port-xxhdpi-screen.png"/>
    <splash qualifier="port-xxxhdpi" src="res/android/splash/drawable-port-xxxhdpi-screen.png"/>
  </platform>
  <platform name="ios">
    <allow-intent href="itms:*"/>
    <allow-intent href="itms-apps:*"/>
    <preference name="BackupWebStorage" value="none"/>
    <preference name="Orientation" value="all"/>
    <icon platform="ios" height="57" src="res/ios/icon/icon.png" width="57"/>
    <icon platform="ios" height="114" src="res/ios/icon/icon@2x.png" width="114"/>
    <icon platform="ios" height="40" src="res/ios/icon/icon-40.png" width="40"/>
    <icon platform="ios" height="80" src="res/ios/icon/icon-40@2x.png" width="80"/>
    <icon platform="ios" height="120" src="res/ios/icon/icon-40@3x.png" width="120"/>
    <icon platform="ios" height="50" src="res/ios/icon/icon-50.png" width="50"/>
    <icon platform="ios" height="100" src="res/ios/icon/icon-50@2x.png" width="100"/>
    <icon platform="ios" height="60" src="res/ios/icon/icon-60.png" width="60"/>
    <icon platform="ios" height="120" src="res/ios/icon/icon-60@2x.png" width="120"/>
    <icon platform="ios" height="180" src="res/ios/icon/icon-60@3x.png" width="180"/>
    <icon platform="ios" height="72" src="res/ios/icon/icon-72.png" width="72"/>
    <icon platform="ios" height="144" src="res/ios/icon/icon-72@2x.png" width="144"/>
    <icon platform="ios" height="76" src="res/ios/icon/icon-76.png" width="76"/>
    <icon platform="ios" height="152" src="res/ios/icon/icon-76@2x.png" width="152"/>
    <icon platform="ios" height="167" src="res/ios/icon/icon-83.5@2x.png" width="167"/>
    <icon platform="ios" height="29" src="res/ios/icon/icon-small.png" width="29"/>
    <icon platform="ios" height="58" src="res/ios/icon/icon-small@2x.png" width="58"/>
    <icon platform="ios" height="87" src="res/ios/icon/icon-small@3x.png" width="87"/>
    <icon platform="ios" height="1024" src="res/ios/icon/icon-1024.png" width="1024"/>
    <splash platform="ios" height="1136" src="res/ios/splash/Default-568h@2x~iphone.png" width="640"/>
    <splash platform="ios" height="1334" src="res/ios/splash/Default-667h.png" width="750"/>
    <splash platform="ios" height="2208" src="res/ios/splash/Default-736h.png" width="1242"/>
    <splash platform="ios" height="1242" src="res/ios/splash/Default-Landscape-736h.png" width="2208"/>
    <splash platform="ios" height="1536" src="res/ios/splash/Default-Landscape@2x~ipad.png" width="2048"/>
    <splash platform="ios" height="2048" src="res/ios/splash/Default-Landscape@~ipadpro.png" width="2732"/>
    <splash platform="ios" height="768" src="res/ios/splash/Default-Landscape~ipad.png" width="1024"/>
    <splash platform="ios" height="2048" src="res/ios/splash/Default-Portrait@2x~ipad.png" width="1536"/>
    <splash platform="ios" height="2732" src="res/ios/splash/Default-Portrait@~ipadpro.png" width="2048"/>
    <splash platform="ios" height="1024" src="res/ios/splash/Default-Portrait~ipad.png" width="768"/>
    <splash platform="ios" height="960" src="res/ios/splash/Default@2x~iphone.png" width="640"/>
    <splash platform="ios" height="480" src="res/ios/splash/Default~iphone.png" width="320"/>
    <splash platform="ios" height="2732" src="res/ios/splash/Default@2x~universal~anyany.png" width="2732"/>
  </platform>
  <preference name="SplashScreen" value="screen"/>
  <preference name="AutoHideSplashScreen" value="true" />
  <preference name="ShowSplashScreenSpinner" value="true"/>
  <preference name="SplashMaintainAspectRatio" value="true"/>
  <preference name="SplashScreenSpinnerColor" value="#ccffff"/>
  <feature name="SplashScreen">
    <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen"/>
  </feature>
</widget>
